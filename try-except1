
import sys
def main():
    print("PROGRAM PEMBAGIAN BILANGAN")
    a = float(input("masukan a:"))
    b = float(input("masukan b:"))
 
    try:
         hasil = a/b

    except ZeroDivisionError:
         print("\nERROR: nilai b tidak boleh nol")
         sys.exit(1)
         
    print("\na: ",a)
    print("b :",b)
    print("a/b= ",hasil)
if __name__ == "__main__":
    main()
