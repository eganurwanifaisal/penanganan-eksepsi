def main():
    print("PROGRAM PEMBAGIAN BILANGAN")
    try:
        a = float(input("masukan a: "))
        b = float(input("masukan b: "))
      
        hasil = a / b
    except(ZeroDivisionError, ValueError,
             KeyboardInterrupt):
        print("\nERROR: anda telah melakukan "+
            "kesalahan")
    else:
        print("\na :",a)
        print("b : ", b)
        print("a / b = ",hasil)
if __name__=="__main__":
  main()
      
